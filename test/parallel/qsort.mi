let append2 = fix (lam append2: [Int] -> [Int] -> [Int].
    	      	    lam l1:[Int]. lam l2:[Int].
		ifexp (isnil l1)
		(lam _:a. l2)
		(lam _:a. cons (head l1) (append2 (tail l1) l2)))
		

